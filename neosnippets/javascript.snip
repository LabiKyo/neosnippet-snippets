snippet f
options word
  function ${1:#:function_name}(${2:#:attribute}) {
    ${0:TARGET}
  }${3:;}

snippet :f
options head
  ${1:#:method_name}: function $1(${2:#:attribute}) {
    ${0:TARGET}
  },

snippet of
abbr object function
options head
  ${1:#:method_name} (${2:#:attribute}) {
    ${0:TARGET}
  },

snippet f);
alias f)
options word
  function ${1:#:function_name}(${2:#:attribute}) {
    ${0:TARGET}
  });

snippet ff);
alias ff)
options word
  function ${1:thenResolve:then_resolve_name}(${2:#:attribute}) {
    ${0:TARGET}
  }, function ${3:thenReject:then_reject_name}(${4:#:attribute}) {
  });

snippet o
options word
  {
    ${0:TARGET}
  }${1:;}

snippet :o
options head
  ${1:#:method_name}: {
    ${0:TARGET}
  },

snippet o);
alias o)
options word
  {
    ${0:TARGET}
  });

snippet a
options word
  [
    ${0:TARGET}
  ]${1:;}

snippet :a
options head
  ${1:#:method_name}: [
    ${0:TARGET}
  ],

snippet a);
alias a)
options word
  [
    ${0:TARGET}
  ]);

snippet function
alias   func
options word
  function $1(${2:#:argument}) {
    ${0:TARGET}
  }

snippet proto
options head
  ${1:#:class_name}.prototype.${2:#:method_name} = function(${3:#:first_argument}) {
    ${0:TARGET}
  };

snippet if
options head
  if (${1:true}) {
    ${0:TARGET}
  }

snippet if-else
alias ife
options head
  if (${1:#:condition}) {
    ${0:TARGET}
  } else {
  }

snippet for
options head
  for (var ${1:i}=0; $1 < ${2:#:Things}.length; ++$1) {
    ${0:TARGET}
  }

snippet forin
options head
  for (var ${1:i} in ${2:#:Things}) {
    ${0:TARGET}
  }

snippet key-value
alias :
options word
  ${1:#:key}: ${0:#:value},

#snippet key
#options word
