snippet setTimeout-function
alias setTo
options head
  setTimeout(function() {
    ${0}
  }, ${1:10});

snippet console-log
alias cl
options head
  console.log(${0:TARGET});

snippet console-error
alias ce
options head
  console.error(${0:TARGET});

snippet console-log'
alias cl'
options head
  console.log('${0:TARGET}');

snippet extend
alias ext
options head
  extend({
    ${0:TARGET}
  });
